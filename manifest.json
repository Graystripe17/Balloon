{
    "manifest_version": 1,
    
    "name": "Balloon",
    "description": "Getting started simply",
    "version": "1.0",
    "manifest_version": 2,
    
    "browser_action": {
        "default_icon": "Balloon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "content_scripts": [{
        "js": ["birthday.js"]
    
    }]
}
